#!/usr/bin/env stargate-shell

# Test bytecode compilation and execution

# Simple arithmetic
let x = 10;
let y = 20;
let z = x + y;
print("10 + 20 = {z}");

# SmallInt optimization
let a = 100;
let b = 50;
let c = a - b;
print("100 - 50 = {c}");

# Conditionals
let num = 15;
if num > 10 {
    print("{num} is greater than 10");
} else {
    print("{num} is not greater than 10");
}

# List building
let numbers = [1, 2, 3, 4, 5];
print("Numbers: {numbers}");
