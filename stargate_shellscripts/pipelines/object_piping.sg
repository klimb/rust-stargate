#!/usr/bin/env stargate-shell
# Test object piping with stargate commands

use ut;

[test]
fn test_piping() {
    let result = list-directory;
    ut.assert_true(result.total_count >= 0, "Pipeline should return object with total_count");
}

[test]
fn test_filter_piping() {
    let result = list-directory | find-text toml;
    ut.assert_true(true, "Filter pipeline should execute");
}

print ut.stats;
exit(ut.healthy);
