#!/usr/bin/env stargate-shell
# Demo: Property completion and access

use ut;

[test]
fn test_property_access() {
    let count = (list-directory).total_count;
    ut.assert_true(count > 0, "Directory should have items");
}

[test]
fn test_array_access() {
    let entries = (list-directory).entries;
    let first = entries[0].name;
    ut.assert_not_equals(first, "", "First entry should have name");
}

print ut.stats;
exit(ut.healthy);
